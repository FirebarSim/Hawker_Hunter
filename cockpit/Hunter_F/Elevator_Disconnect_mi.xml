<Gauge Name="Elevator Disconnect MI" Version="1.0">
   <!--Saverio Maurri 2004-->
   <Element>
      <Select>
         <Value>(L:Elevator_Power,bool)</Value>
         <Case Value="0">
            <Image Name="MI_Black.bmp"/>
         </Case>
         <Case Value="1">
            <Image Name="MI_White.bmp"/>
         </Case>
      </Select>
   </Element>
   <Element>
      <Select>
         <Value>(L:Elevator_Power,bool) 1 == if{ 0 (&gt;K:ELEVATOR_SET) }</Value>
      </Select>
   </Element>
   <Mouse>
      <Tooltip>Elevator Power MI</Tooltip>
   </Mouse>
</Gauge>
