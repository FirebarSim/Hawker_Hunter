<Gauge Name="Airbrake warning MI" Version="1.1">
   <!--Saverio Maurri 2004-->
   <Image Name="Airbrake_warnMI_off.bmp"/>
   <Element>
      <Position X="0" Y="0"/>
      <Select>
         <Value>(A:Spoilers handle position, percent) </Value>
         <Failures>
            <SYSTEM_ELECTRICAL_PANELS Action="0"/>
         </Failures>
         <Case Value="100">
            <Image Name="Airbrake_warnMI_on.bmp" Luminous="1"/>
         </Case>
      </Select>
   </Element>
   <Mouse>
      <Tooltip>Airbrake Warn (Black when fully IN)</Tooltip>
   </Mouse>
</Gauge>
