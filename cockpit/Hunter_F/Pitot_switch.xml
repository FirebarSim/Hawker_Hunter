<Gauge Name="Pitot Heat" Version="1.0">
   <Element>
      <Select>
         <Value>(A:Pitot heat,bool)</Value>
         <Case Value="0">
            <Image Name="SPST_DWN.bmp" Luminous="1"/>
         </Case>
         <Case Value="1">
            <Image Name="SPST_UP.bmp" Luminous="1"/>
         </Case>
      </Select>
   </Element>
   <Mouse>
      <Tooltip ID="TOOLTIPTEXT_PITOT_HEAT_SWITCH"/>
      <Cursor Type="Hand"/>
      <Click Event="PITOT_HEAT_TOGGLE"/>
   </Mouse>
</Gauge>
