      </Select>
   </Element>
   <Element>
      <Position X="0" Y="0"/>
      <Select>
         <Value>(A:NAV1 TOFROM, ENUM)</Value>
         <Case Value="1">
            <Image Name="TO_Arrow.bmp" ImageSizes="320,320,320,320"/>
         </Case>
         <Case Value="2">
            <Image Name="FROM_Arrow.bmp" ImageSizes="320,320,320,320"/>
         </Case>
      </Select>
   </Element>
   <Element>
      <Position X="0" Y="0"/>
      <Select>
         <Value>(A:NAV1 Has NAV, bool)</Value>
         <Failures>
            <SYSTEM_ELECTRICAL_PANELS Action="0"/>
         </Failures>
         <Case Value="0">
            <Image Name="NO_VOR_flag.bmp" ImageSizes="320,320,320,320"/>
         </Case>
      </Select>
   </Element>
   <Element>
      <Position X="160" Y="160"/>
      <Image Name="HDG_card.bmp" ImageSizes="320,320,320,320">
         <Axis X="160" Y="160"/>
      </Image>
      <Rotate>
         <Value>(A:NAV1 OBS,radians) -1 *</Value>
      </Rotate>
   </Element>
   <Element>
      <Position X="164.5" Y="84"/>
      <Select>
         <Value>(A:NAV1 has NAV, bool) 0 !=</Value>
         <Image Name="VOR_pointer.bmp" ImageSizes="140,8,140,8" PointsTo="North" Luminous="1">
            <Axis X="0" Y="0"/>
         </Image>
      </Select>
      <Rotate>
         <Value>(A:NAV1 CDI,number) 210 / -1 *</Value>
      </Rotate>
   </Element>
   <Element>
      <Position X="90" Y="156"/>
      <Select>
         <Value>(A:NAV1 has NAV, bool) 0 !=</Value>
         <Image Name="GS_pointer.bmp" ImageSizes="135,8,135,8" PointsTo="East" Luminous="1">
            <Axis X="0" Y="0"/>
         </Image>
      </Select>
      <Rotate>
         <Value>(A:NAV1 GSI,number) 300 /</Value>
      </Rotate>
   </Element>
   <Element>
      <Position X="37" Y="276"/>
      <Image Name="OBS_knob.bmp" ImageSizes="58,58,58,58">
         <Axis X="29" Y="29"/>
      </Image>
      <Rotate>
         <Value>(A:NAV1 OBS,radians) 3 *</Value>
      </Rotate>
      <Nonlinearity/>
   </Element>
   <Mouse>
      <Area Top="260" Left="10" Width="50" Height="40">
         <Tooltip ID="TOOLTIPTEXT_HSI_HEADING_COURSE"/>
         <Area Right="20">
            <Cursor Type="DownArrow"/>
            <Click Event="VOR1_OBI_DEC" Repeat="Yes"/>
         </Area>
         <Area Left="20">
            <Cursor Type="UpArrow"/>
            <Click Event="VOR1_OBI_INC" Repeat="Yes"/>
         </Area>
      </Area>
   </Mouse>
</Gauge>
