   </Element>
   <Element>
      <Position X="0" Y="0"/>
      <Select>
         <Value>(A:NAV1 has Glide Slope, bool)</Value>
         <Failures>
            <SYSTEM_ELECTRICAL_PANELS Action="0"/>
         </Failures>
         <Case Value="0">
            <Image Name="NO_GS_flag.bmp" ImageSizes="320,320,320,320"/>
         </Case>
         <Case Value="1">
            <Image Name="GS_flag.bmp" ImageSizes="320,320,320,320"/>
