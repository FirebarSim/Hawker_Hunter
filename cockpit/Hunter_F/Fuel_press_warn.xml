<Gauge Name="Fuel pressure Warning" Version="1.0">
   <!--Saverio Maurri 2004-->
   <Element>
      <Select>
         <Value>(A:General Eng Fuel Pressure:1,psi) 3.5 &lt;</Value>
         <Failures>
            <SYSTEM_ELECTRICAL_PANELS Action="0"/>
         </Failures>
         <Case Value="1">
            <Image Name="Cabinalt_warn_on.bmp" Bright="Yes"/>
         </Case>
      </Select>
   </Element>
</Gauge>
