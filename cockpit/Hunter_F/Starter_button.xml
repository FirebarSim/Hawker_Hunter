<Gauge Name="Starter button" Version="1.1">
   <Element>
      <Select>
         <Value> (A:General eng1 starter, bool)</Value>
         <Case Value="0">
            <Image Name="Start_button_off.bmp"/>
         </Case>
         <Case Value="1">
            <Image Name="Start_button_on.bmp"/>
         </Case>
      </Select>
   </Element>
   <Mouse>
      <Tooltip>Starter Button</Tooltip>
      <Cursor Type="Hand"/>
      <Click Event="TOGGLE_STARTER1" Repeat="Yes"/>
   </Mouse>
</Gauge>
