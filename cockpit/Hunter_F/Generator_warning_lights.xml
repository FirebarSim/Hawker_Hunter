<Gauge Name="Oil temperature warning" Version="1.0">
   <Image Name="Generator_warn_bg.bmp"/>
   <Element>
      <Position X="0" Y="0"/>
      <Select>
         <Value>(A:Electrical genalt bus voltage:1, volts) 20 &lt;=</Value>
         <Case Value="1">
            <Image Name="Generator_warn_on.bmp" Bright="Yes"/>
         </Case>
      </Select>
   </Element>
   <Element>
      <Position X="0" Y="0"/>
      <Select>
         <Value>(A:Eng2 oil temperature,celsius) 115 &gt; </Value>
         <Case Value="1">
            <Image Name="Generator_warn_on.bmp" Bright="Yes"/>
         </Case>
      </Select>
   </Element>
   <Mouse>
      <Tooltip>Generator Voltage Warning</Tooltip>
   </Mouse>
</Gauge>
