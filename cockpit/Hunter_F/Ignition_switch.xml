<Gauge Name="Ignition Switch" Version="1.1">
   <Element>
      <Select>
         <Value>(A:GENERAL ENG1 FUEL VALVE,BOOL)</Value>
         <Case Value="0">
            <Image Name="SPST_DWN.bmp" Luminous="1"/>
         </Case>
         <Case Value="1">
            <Image Name="SPST_UP.bmp" Luminous="1"/>
         </Case>
      </Select>
   </Element>
   <Mouse>
      <Tooltip>Ignition Switch</Tooltip>
      <Cursor Type="Hand"/>
      <Click Event="TOGGLE_FUEL_VALVE_ENG1"/>
   </Mouse>
</Gauge>
